{
  "name": "premail",
  "version": "2.0.0",
  "description": "An easy-to-use component-based build system for MJML, the email templating language, with Handlebars templating, Sass styles, and extra options.",
  "bin": {
    "premail": "./index.js"
  },
  "main": "index.js",
  "files": [
    "/docs",
    "/src"
  ],
  "scripts": {
    "start": "gulp build",
    "test": "modern test --coverage --coverageReporters='text'",
    "format": "modern format && gulp formatTemplates",
    "lint": "modern lint",
    "precommit": "modern precommit",
    "readme": "markdown-exec ./README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/premail/premail.git"
  },
  "engines": {
    "node": "^12 || ^14 || ^16",
    "npm": "^7 || ^8"
  },
  "homepage": "https://premail.dev",
  "keywords": [
    "email",
    "email generator",
    "email marketing",
    "email templates",
    "email templating",
    "gulp",
    "gulp 4",
    "handlebars",
    "mjml",
    "mjml 4",
    "mjml framework",
    "mjml template",
    "mjml to html",
    "sass"
  ],
  "author": "Ivan Boothe <npm@rootwork.org> (https://rootwork.org/)",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/premail/premail/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/rootwork"
  },
  "dependencies": {
    "ansi-colors": "^4.1.1",
    "ansi-colors-prioritized": "^0.1.2",
    "del": "^6.0.0",
    "detergent": "^7.1.0",
    "fancy-log": "^1.3.3",
    "fibers": "^5.0.0",
    "filesize": "^8.0.0",
    "fs-extra": "^10.0.0",
    "gulp": "^4.0.2",
    "gulp-if": "^3.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.0.0",
    "gulp-tap": "^2.0.0",
    "handlebars": "^4.7.7",
    "html-crush": "^4.2.0",
    "html-img-alt": "^2.1.0",
    "html-to-text": "^8.0.0",
    "js-yaml": "^4.1.0",
    "map-stream": "0.0.7",
    "mjml": "^4.10.3",
    "mjml-bullet-list": "^1.1.6",
    "mjml-signoff": "^1.1.3",
    "mri": "^1.1.6",
    "node-sass-json-importer": "^4.3.0",
    "npm": "^6.14.14",
    "plugin-error": "^1.0.1",
    "prettier": "^2.4.1",
    "pretty-error": "^3.0.4",
    "prompts": "^2.4.2",
    "sass": "^1.39.0",
    "string-strip-html": "^8.3.0",
